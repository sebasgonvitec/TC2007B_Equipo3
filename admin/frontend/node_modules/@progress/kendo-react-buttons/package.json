{
  "name": "@progress/kendo-react-buttons",
  "version": "5.6.0",
  "description": "All you need in React Button in one package: disabled/enabled states, built-in styles and more. KendoReact Buttons package",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-react.git"
  },
  "main": "dist/npm/main.js",
  "types": "dist/npm/main.d.ts",
  "module": "dist/es/main.js",
  "jsnext:main": "dist/es/main.js",
  "scripts": {
    "test": "cd ../../ && npm run test -- --testPathPattern=/packages/buttons/.*",
    "test:chip": "cd ../../ && npm run test -- --testPathPattern=/packages/buttons/test/chip/.*",
    "e2e": "cd ../../ && npx jest --maxWorkers=4 --config jest.e2e.js packages/buttons",
    "e2e-axe": "cd ../../ && npx jest --maxWorkers=4 --config jest.e2e.js packages/buttons/e2e/a11y.tests.ts",
    "start": "gulp start",
    "build-package": "gulp build-package"
  },
  "homepage": "https://www.telerik.com/kendo-react-ui",
  "keywords": [
    "Kendo UI",
    "React",
    "Progress",
    "Buttons",
    "KendoReact",
    "reactjs",
    "UI",
    "components",
    "React component",
    "Telerik"
  ],
  "peerDependencies": {
    "@progress/kendo-licensing": "^1.0.1",
    "react": "^16.8.2 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.2 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@progress/kendo-react-common": "5.6.0",
    "@progress/kendo-react-popup": "5.6.0",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "@progress/kendo-drawing": "^1.5.12",
    "@progress/kendo-licensing": "^1.0.1",
    "@progress/kendo-react-animation": "5.6.0",
    "@progress/kendo-react-dialogs": "5.6.0",
    "@progress/kendo-react-dropdowns": "5.6.0",
    "@progress/kendo-react-form": "5.6.0",
    "@progress/kendo-react-inputs": "5.6.0",
    "@progress/kendo-react-intl": "5.6.0",
    "@progress/kendo-react-labels": "5.6.0",
    "@progress/kendo-react-layout": "5.6.0",
    "@progress/kendo-react-ripple": "5.6.0",
    "@progress/kendo-svg-icons": "^0.1.0"
  },
  "@progress": {
    "friendlyName": "Buttons",
    "framework": "KendoReact"
  },
  "author": "Progress",
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
