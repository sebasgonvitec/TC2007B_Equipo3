{
  "name": "@progress/kendo-licensing",
  "version": "1.2.2",
  "description": "Kendo UI License Manager",
  "author": "Progress",
  "license": "SEE LICENSE IN LICENSE.md",
  "bin": {
    "kendo-ui-license": "./bin/kendo-ui-license.js"
  },
  "main": "dist/index.js",
  "module": "dist/index-esm.js",
  "typings": "dist/index.d.ts",
  "jsdelivr": "dist/index.js",
  "unpkg": "dist/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint src",
    "test": "jest",
    "test:watch": "jest --watch",
    "semantic-release": "semantic-release pre && semantic-prerelease publish --public && semantic-release post",
    "postinstall": "node ./bin/kendo-ui-license.js activate --ignore-no-license"
  },
  "dependencies": {
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@progress/kendo-common-tasks": "^7.4.3",
    "@rollup/plugin-strip": "^2.0.1",
    "@telerik/semantic-prerelease": "^1.3.3",
    "@types/jest": "^26.0.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^10.17.27",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "ghooks": "^2.0.4",
    "husky": "^4.2.5",
    "javascript-obfuscator": "^1.5.0",
    "jest": "^26.1.0",
    "jest-preset-typescript": "^1.2.0",
    "rollup": "^2.21.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "semantic-release": "^6.3.6",
    "ts-jest": "^26.1.1",
    "tslib": "^2.0.0",
    "typescript": "~3.9.6"
  },
  "jest": {
    "preset": "jest-preset-typescript"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-licensing.git"
  },
  "release": {
    "debug": false,
    "branchTags": {
      "develop": "dev"
    },
    "fallbackTags": {
      "dev": "latest"
    },
    "analyzeCommits": "@telerik/semantic-prerelease/analyzeCommits",
    "generateNotes": "@telerik/semantic-prerelease/generateNotes",
    "getLastRelease": "@telerik/semantic-prerelease/getLastRelease",
    "verifyConditions": "@telerik/semantic-prerelease/verifyConditions",
    "verifyRelease": "@telerik/semantic-prerelease/verifyRelease"
  },
  "files": [
    "bin",
    "dist",
    "index.d.ts"
  ]
}