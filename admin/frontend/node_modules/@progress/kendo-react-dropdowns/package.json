{
  "name": "@progress/kendo-react-dropdowns",
  "version": "5.6.0",
  "description": "React DropDowns offer an interface for users to select different items from a list and more. KendoReact Dropdowns package",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-react.git"
  },
  "main": "dist/npm/main.js",
  "types": "dist/npm/main.d.ts",
  "module": "dist/es/main.js",
  "jsnext:main": "dist/es/main.js",
  "scripts": {
    "test": "cd ../../ && npm run test -- --testPathPattern=/packages/dropdowns/.*",
    "test:ac": "cd ../../ && npm run test -- --testPathPattern=/packages/dropdowns/test/autocomplete/.*",
    "test:combo": "cd ../../ && npm run test -- --testPathPattern=/packages/dropdowns/test/combobox/.*",
    "test:dd": "cd ../../ && npm run test -- --testPathPattern=/packages/dropdowns/test/dropdownlist/.*",
    "test:dropdowntree": "cd ../../ && npm run test -- --testPathPattern=/packages/dropdowns/test/dropdowntree/.*",
    "test:multicombo": "cd ../../ && npm run test -- --testPathPattern=/packages/dropdowns/test/multicolumncobmobox/.*",
    "test:common": "cd ../../ && npm run test -- --testPathPattern=/packages/dropdowns/test/common/.*",
    "e2e": "cd ../../ && npx jest --maxWorkers=4 --config jest.e2e.js packages/dropdowns",
    "e2e-axe": "cd ../../ && npx jest --maxWorkers=4 --config jest.e2e.js packages/dropdowns/e2e/a11y.tests.ts",
    "start": "gulp start",
    "lint": "gulp lint",
    "build-package": "gulp build-package"
  },
  "homepage": "https://www.telerik.com/kendo-react-ui",
  "keywords": [
    "Kendo UI",
    "React",
    "KendoReact",
    "reactjs",
    "UI",
    "components",
    "React component",
    "Telerik",
    "Dropdowns",
    "AutoComplete",
    "ComboBox",
    "DropDownList",
    "MultiSelect"
  ],
  "peerDependencies": {
    "@progress/kendo-licensing": "^1.0.1",
    "@progress/kendo-react-buttons": "^5.0.0",
    "@progress/kendo-react-intl": "^5.0.0",
    "@progress/kendo-react-treeview": "^5.0.0",
    "react": "^16.8.2 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.2 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@progress/kendo-react-common": "5.6.0",
    "@progress/kendo-react-labels": "5.6.0",
    "@progress/kendo-react-popup": "5.6.0",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "@progress/kendo-data-query": "^1.0.0",
    "@progress/kendo-date-math": "^1.4.1",
    "@progress/kendo-drawing": "^1.8.0",
    "@progress/kendo-licensing": "^1.0.1",
    "@progress/kendo-react-animation": "5.6.0",
    "@progress/kendo-react-buttons": "5.6.0",
    "@progress/kendo-react-data-tools": "5.6.0",
    "@progress/kendo-react-dateinputs": "5.6.0",
    "@progress/kendo-react-grid": "5.6.0",
    "@progress/kendo-react-inputs": "5.6.0",
    "@progress/kendo-react-intl": "5.6.0",
    "@progress/kendo-react-treeview": "5.6.0"
  },
  "@progress": {
    "friendlyName": "Dropdowns",
    "framework": "KendoReact"
  },
  "author": "Progress",
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
