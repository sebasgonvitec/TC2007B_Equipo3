{
  "name": "@progress/kendo-react-dateinputs",
  "description": "React DateInput is a perfect input component for handling quick and efficient date values. KendoReact Date Inputs package",
  "version": "5.6.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-react.git"
  },
  "main": "dist/npm/main.js",
  "types": "dist/npm/main.d.ts",
  "module": "dist/es/main.js",
  "jsnext:main": "dist/es/main.js",
  "scripts": {
    "test": "cd ../../ && npm run test -- --testPathPattern=/packages/dateinputs/.*",
    "test:dateinput": "cd ../../ && npm run test -- --testPathPattern=/packages/dateinputs/test/dateinput/.*",
    "test:datepicker": "cd ../../ && npm run test -- --testPathPattern=/packages/dateinputs/test/datepicker/.*",
    "test:timepicker": "cd ../../ && npm run test -- --testPathPattern=/packages/dateinputs/test/timepicker/.*",
    "test:datetimepicker": "cd ../../ && npm run test -- --testPathPattern=/packages/dateinputs/test/datetimepicker/.*",
    "e2e": "cd ../../ && npx jest --maxWorkers=4 --config jest.e2e.js packages/dateinputs/e2e",
    "e2e-axe": "cd ../../ && npx jest --maxWorkers=4 --config jest.e2e.js packages/dateinputs/e2e/accessibility/tests.ts",
    "start": "gulp start",
    "build-package": "gulp build-package"
  },
  "homepage": "https://www.telerik.com/kendo-react-ui",
  "keywords": [
    "Kendo UI",
    "React",
    "Progress",
    "Date Inputs",
    "KendoReact",
    "reactjs",
    "UI",
    "components",
    "React component",
    "DatePicker",
    "form valildation",
    "date",
    "picker",
    "datetime",
    "calendar",
    "Telerik"
  ],
  "peerDependencies": {
    "@progress/kendo-licensing": "^1.0.1",
    "@progress/kendo-react-intl": "^5.0.0",
    "react": "^16.8.2 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.2 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@progress/kendo-date-math": "^1.4.0",
    "@progress/kendo-react-buttons": "5.6.0",
    "@progress/kendo-react-common": "5.6.0",
    "@progress/kendo-react-labels": "5.6.0",
    "@progress/kendo-react-popup": "5.6.0",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "@progress/kendo-data-query": "^1.5.5",
    "@progress/kendo-drawing": "^1.15.0",
    "@progress/kendo-licensing": "^1.0.1",
    "@progress/kendo-react-animation": "5.6.0",
    "@progress/kendo-react-data-tools": "5.6.0",
    "@progress/kendo-react-dropdowns": "5.6.0",
    "@progress/kendo-react-grid": "5.6.0",
    "@progress/kendo-react-inputs": "5.6.0",
    "@progress/kendo-react-intl": "5.6.0",
    "@progress/kendo-react-tooltip": "5.6.0"
  },
  "@progress": {
    "friendlyName": "Date Inputs",
    "framework": "KendoReact"
  },
  "author": "Progress",
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
