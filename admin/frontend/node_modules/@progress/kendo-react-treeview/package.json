{
  "name": "@progress/kendo-react-treeview",
  "version": "5.6.0",
  "description": "React TreeView displays hierarchical data in a traditional tree structure, supports user interaction. KendoReact TreeView package",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-react.git"
  },
  "main": "dist/npm/main.js",
  "types": "dist/npm/main.d.ts",
  "module": "dist/es/main.js",
  "jsnext:main": "dist/es/main.js",
  "scripts": {
    "test": "cd ../../ && npm run test -- --testPathPattern=/packages/treeview/.*",
    "e2e": "cd ../../ && npx jest --maxWorkers=4 --config jest.e2e.js packages/treeview",
    "e2e-axe": "cd ../../ && npx jest --maxWorkers=4 --config jest.e2e.js packages/treeview/e2e/accessibility.tests.ts",
    "start": "gulp start",
    "build-package": "gulp build-package",
    "ssr": "gulp bundle-ssr-server bundle-ssr-client && node dist/ssr/server.js"
  },
  "homepage": "https://www.telerik.com/kendo-react-ui",
  "keywords": [
    "Kendo UI",
    "React",
    "Progress",
    "TreeView",
    "KendoReact",
    "reactjs",
    "UI",
    "components",
    "React component",
    "Telerik"
  ],
  "peerDependencies": {
    "@progress/kendo-licensing": "^1.0.1",
    "@progress/kendo-react-animation": "^5.0.0",
    "react": "^16.8.2 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.2 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@progress/kendo-react-common": "5.6.0",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "@progress/kendo-licensing": "^1.0.1",
    "@progress/kendo-react-animation": "5.6.0",
    "@progress/kendo-react-dropdowns": "5.6.0",
    "@progress/kendo-react-intl": "5.6.0"
  },
  "@progress": {
    "friendlyName": "TreeView",
    "framework": "KendoReact"
  },
  "author": "Progress",
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
